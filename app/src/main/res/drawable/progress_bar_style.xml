<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Фон: прозрачный -->
    <item android:id="@android:id/background">
        <shape
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:shape="rectangle">
            <corners android:radius="10dp" /> <!-- Здесь вы можете задать радиус закругления -->
            <solid android:color="@android:color/transparent" /> <!-- Прозрачный цвет фона -->
            <stroke
                android:width="1dp"
                android:color="@color/black" />
        </shape>
    </item>

    <!-- Прогресс: зеленый -->
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="10dp" /> <!-- Здесь вы можете задать радиус закругления -->
                <solid android:color="@android:color/holo_green_dark" /> <!-- Зеленый цвет прогресса -->
                <stroke android:width="1dp"
                    android:color="@color/black"/>
            </shape>
        </clip>
    </item>

</layer-list>